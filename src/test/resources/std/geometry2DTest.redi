import geometry2D

fun main() {
    testPoint()
}

fun testPoint() {
    m1 = Point(x=5, y=5)
    m2 = Point(x=2, y=1)
    minus_res = m1.minus(m2)
    test(minus_res.x == 3)
    test(minus_res.y == 4)

    rotated = Point(y = 1)
    test(rotated.y == 1)
    rotateResult = rotated.rotate(90)
    test(rotateResult.x == -1)
    test(floatEquals(rotateResult.y, 0))
}