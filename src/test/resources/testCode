class Root {
segm = Segment()
iter = 0
}

class Segment{
c = a + b + a
iter = 1
a = 5
b = a
}

fun toDegrees(rad) {
return
a=b.c.d.e.f().g.h.parent.(if(a==true) u else b).t
}


class Math {
    RANDOM_SEED = 42
    PI = 3.1415926
}

fun rotate(point, pivot, angle) {
break}

fun abs(x) {
    return if (x < 0) -x else x
}

fun toRadians(deg) {
    return abs( \
    -deg) * 3 / 180
}

// returns random int in range [a,b]
fun rndInt(a,b)  {
    t = int(rnd() * ((b-a)+1))
    log("t:"+t)
    log(t)
    return a + t
}

// get all classes of classRef type
fun all(classRef) {
    res = []
    stack = [Root]
    while(stack.size > 0) {
        current = stack.pop()
        if(current is classRef) {
            res.add(current)
        } else {
        }
        }
        stack.add(res.properties !is Value)
    }
}//
