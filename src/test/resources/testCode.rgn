fun checkMultipleCalls(a)
{
    b = a + 0
    if(a > 1)
    {
        res = 1 + checkMultipleCalls(a - 1)
    } else {
        res = 1
    }
    test(a == b)
    log("res: " +res + ", a: "+ a)
    return res
}

